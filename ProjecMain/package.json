{
    "name": "moder_space",
    "version": "0.0.0",
    "description": "E-commerce shop!",
    "main": "index.js",
    "scripts": {
        "start": "echo \"App has been started\" && exit 1",
        "start:dev": "webpack serve -c config/webpack.development.config.js",
        "build": "webpack build -c config/webpack.production.config.js",
        "build:dev": "webpack build -c config/webpack.development.config.js",
        "lint": "eslint --ext .js,.jsx --max-warnings 0 ./src",
        "lint:fix": "eslint --fix src",
        "test": "jest",
        "test:watch": "jest --watch",
        "prepare": "husky install",
        "test:ct": "cypress run --component --browser chrome",
        "test:open": "cypress open",
        "format": "prettier --write \"src/**/*.{js,jsx,json,css,html}\"",
        "prettier:check": "prettier --check \"src/**/*.{js,jsx,json,css,html}\"",
        "nyc": "nyc --reporter=html --exclude '**/*.cy.*' --include 'src/**/*'"
    },
    "keywords": [
        "react",
        "react-dom"
    ],
    "author": "albinapetrus",
    "license": "ISC",
    "dependencies": {
        "firebase": "^11.0.1",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-icons": "^5.3.0",
        "react-router-dom": "^6.27.0",
        "reacticons": "^0.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-react": "^7.25.9",
        "@cypress/code-coverage": "^3.13.2",
        "@istanbuljs/nyc-config-babel": "^3.0.0",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.2.1",
        "babel-plugin-istanbul": "^7.0.0",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^7.1.2",
        "cypress": "^13.15.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-cypress": "^3.5.0",
        "eslint-plugin-import": "^2.30.0",
        "eslint-plugin-react": "^7.37.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unicorn": "^55.0.0",
        "html-webpack-plugin": "^5.6.3",
        "husky": "^8.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^15.2.10",
        "mini-css-extract-plugin": "^2.9.1",
        "nyc": "^17.1.0",
        "prettier": "^3.3.3",
        "style-loader": "^4.0.0",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0",
        "webpack-merge": "^6.0.1"
    }
}
